<%- include("partials/header")-%>
<h1>Compose</h1>
<form method="post" action="/compose">
  <div class="form-group">
    <label style="font-size:30px">Title</label>
    <input name="PostTitle" type="text" class="form-control" placeholder="Enter Title">
  </div>
  <div class="form-group">
    <label style="font-size:30px">Post</label>
    <textarea class="form-control" name="PostBody" rows="4" cols="101" placeholder="Write Something"></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-info">Publish</button>
</form>
<%- include("partials/footer") -%>
